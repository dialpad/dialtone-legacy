    {
      "class": "normal",
      "description": "Lines will only break at normal break points such as a space between two words."
    },
    {
      "class": "anywhere",
      "description": "To keep copy from overflowing its content box, line breaks are inserted at any point in a line. No hyphenation character is inserted at the break point."
    },
    {
      "class": "break-word",
      "description": "The same as the anywhere value, except soft wrap opportunities are not considered when calculating min-content intrinsic sizes."
    },
    {
      "class": "unset",
      "description": "Resets to the initial, default line break rule."
    }
  ],
